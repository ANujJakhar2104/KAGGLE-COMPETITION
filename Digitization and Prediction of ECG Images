    "def split_into_leads(img):\n",
    "    h, w, _ = img.shape\n",
    "    lead_h = h // 12\n",
    "    leads = []\n",
    "\n",
    "    for i in range(12):\n",
    "        y1 = int(i * lead_h + 0.1 * lead_h)\n",
    "        y2 = int((i + 1) * lead_h - 0.1 * lead_h)\n",
    "        leads.append(img[y1:y2, :])\n",
    "\n",
    "    return leads"
   ]
  },
