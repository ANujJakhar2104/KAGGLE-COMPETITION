    "def process_ecg_image(img):\n",
    "    lead_imgs = split_into_leads(img)\n",
    "    signals = []\n",
    "\n",
    "    for lead_img in lead_imgs:\n",
    "        bw = preprocess_image(lead_img)\n",
    "        sig = extract_waveform(bw)\n",
    "        signals.append(sig)\n",
    "\n",
    "    return signals"
   ]
  },
